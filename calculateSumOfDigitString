def digitSum(s, k):
       
        while len(s) > k:
            res = []
            total = 0
            for i in range(0,len(s), k):
                number = s[i:i+k]
                total = sum(int(digit) for digit in number)
                res.append(str(total))  
            s = ''.join(res)
        return s

s = "11111222223", k = 3
print(digitSum(s,k))