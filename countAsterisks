def countAsterisks(self, s):
        """
        :type s: str
        :rtype: int
        """
        line = cnt = 0
        for i in s:
            if i == '|':
                line += 1
            if line % 2 == 0:
                if i == '*':
                    cnt +=1
        return cnt

s = "l|*e*et|c**o|*de|"
print(countAsterisks(s))