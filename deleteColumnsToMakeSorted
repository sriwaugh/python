def minDeletionSize(self, strs):
        """
        :type strs: List[str]
        :rtype: int
        """
        dele = 0

        for j in range(len(strs[0])):  
            for i in range(len(strs) - 1):  
                if strs[i][j] > strs[i + 1][j]:
                    dele += 1
                    break
        return dele

strs = ["cba","daf","ghi"]
print(minDeletionSize(strs))